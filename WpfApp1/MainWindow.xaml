<Window x:Name="MainWindow1" x:Class="WpfApp1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp1"
        mc:Ignorable="d"
        Title="芜湖市福利院（随机选号软件）" Height="450" Width="800" Loaded="Window_Loaded" Icon="img/main.png" ResizeMode="NoResize" IsManipulationEnabled="True" WindowStartupLocation="CenterScreen">
    <Window.Background>
        <ImageBrush ImageSource="bg.jpg"/>
    </Window.Background>
    <Menu HorizontalAlignment="Left" Margin="0" VerticalAlignment="Top" Width="793.357">
        <Button x:Name="BtnSet" Content="设置(S)" Width="60" BorderBrush="{x:Null}" Background="{x:Null}" Click="BtnSet_Click"/>
        <Button x:Name="BtnScn" Content="滚屏(E)" Width="60" Background="{x:Null}" BorderBrush="{x:Null}" Click="BtnScn_Click"/>
        <Button x:Name="BtnSup" Content="选项(D)" Background="{x:Null}" BorderBrush="{x:Null}" Width="59" Click="BtnSup_Click"/>
        <Button x:Name="BtnAbt" Content="关于(H)" Width="60" Background="{x:Null}" BorderBrush="{x:Null}" Click="BtnAbt_Click"/>
    </Menu>
</Window>
